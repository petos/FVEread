TIMEDATE_YYYYMM=$(date +%Y%m)
TIMEDATE_YYYYMMDD=$(date +%Y%m%d)

#TIMEDATE_YYYYMMDDHH=$(date +%Y%m%d%H)
TIMEDATE_YYYYMMDDHHmm=$(date +%Y%m%d%H%M)

function debugPrint() {
	if [ $DEBUG ]; then
		echo "date +%H:%M:%S: $1"
	fi
}

function verbPrint() {
	if [ $VERB ]; then
		echo "`date +%H:%M:%S`: $1"
	fi
}

function errorPrint() {
	if [ $VERB ]; then
		echo "$TIMEDATE_YYYYMMDDHHmm: $1"
	fi
}
